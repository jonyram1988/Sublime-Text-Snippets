	public function put(\$datos,\$condiciones){
		if(is_array(\$datos) && is_array(\$condiciones)):
			\$this->_name = '${1:this}';
			return (\$this->update(\$datos,\$condiciones))?true:false;
		else: return false; endif;
	}
]]></content>
